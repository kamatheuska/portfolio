{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "deploy": "git push heroku master",
        "dev": "nodemon server.js",
        "start": "node server.js",
        "test": "tape tests/**/**.*.js | ./node_modules/.bin/tap-spec",
