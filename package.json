{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "deploy": "git push heroku master",
        "heroku-prebuild": "npm run install:view",
        "heroku-postbuild": "npm run build",
        "build": "npm run lint && npm test && npm run build:view",
        "build:view": "./view/buildView.sh",
        "install:view": "./view/installDependencies.sh",
        "dev": "nodemon app.js",
        "dev:debug-log-mode": "DEBUG_MODE=true nodemon app.js",
        "start": "node app.js",
        "test:jest": "jest",
        "test": "tape tests/**/**.test.js | tap-spec",
        "test:watch": "nodemon --exec 'tape tests/**/**.test.js | tap-spec'",
        "test:integration": "tape tests/integration/**.*.js | tap-spec",
        "test:unit": "tape tests/unit/**.*.js | tap-spec",
        "clean": "rm -rf node_modules",
        "debug": "node --inspect app.js",
        "debug:watch": "nodemon --inspect app.js",
        "lint": "eslint --ext .js \"**/*.js\"",
        "lint:fix": "eslint --ext .js \"**/*.js\" --fix"
    },
    "dependencies": {
        "@mapbox/mapbox-sdk": "^0.4.1",
        "axios": "^0.21.1",
        "bluebird": "^3.7.2",
        "body-parser": "^1.18.3",
        "dotenv": "^10.0.0",
        "express": "^4.16.3",
        "lodash": "^4.17.21",
        "mongo": "^0.1.0",
        "mongoose": "^5.12.7",
        "morgan": "^1.10.0",
        "movie-quotes": "^1.0.1",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "tap-spec": "^5.0.0",
        "tape": "^4.13.3"
    },
    "devDependencies": {
        "@types/jest": "^26.0.24",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.0.6",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.2",
        "semver": "^5.3.0",
        "supertest": "^3.4.2",
        "url-loader": "^1.0.1"
    },
    "engines": {
        "node": "10.16.0",
        "npm": "6.9.0"
    },
    "author": "Nicolas Ramirez",
    "license": "ISC"
}
