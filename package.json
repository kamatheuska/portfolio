        "mongoose": "^5.9.7",
        "morgan": "^1.9.0",
        "movie-quotes": "^1.0.0",
        "request": "^2.87.0",
        "request-promise-native": "^1.0.5",
        "supertest": "^3.1.0",
        "tap-spec": "^5.0.0",
        "tape": "^4.9.1"
    },
    "devDependencies": {
        "nodemon": "^1.19.2",
        "semver": "^5.3.0",
        "url-loader": "^1.0.1"
    },
