{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build:view": "npm run build:landing && npm run build:react-projects",
        "build:landing": "cd \"$MAIN/view/landing\" && npm run build",
        "build:react-projects": "cd \"$MAIN/view/react-projects\" && npm run build",
        "build": ". ./setMainFolder.sh && . ./buildEnvs.sh && npm run build:view",
        "clean": "rm -rf node_modules",
        "debug:watch": "nodemon --inspect init.js",
        "debug": "node --inspect init.js",
        "predeploy:prod": "npm run prebuild",
        "deploy:stage": "git push stage develop:master",
        "deploy:prod": "git push prod master",
        "dev:debug-log-mode": "DEBUG_MODE=true nodemon init.js",
        "dev": "nodemon init.js",
        "heroku-postbuild": "npm run build",
        "heroku-prebuild": "npm run install-view",
        "install-view": ". ./setMainFolder.sh && . ./installEnvs.sh && npm run install-view:all",
        "install-view:all": "npm run install-view:landing && npm run install-view:react-projects",
        "install-view:landing": "cd \"$MAIN/view/landing\" && npm install --production=false",
        "install-view:react-projects": "cd \"$MAIN/view/react-projects\" && npm install --production=false",
        "lint:fix": "eslint --ext .js \"**/*.js\" --fix",
        "lint": "eslint --ext .js \"**/*.js\"",
        "prebuild": "npm run lint && npm test",
        "start": "node init.js",
        "test:all": "npm run test:unit && npm run test:integration",
        "test:integration": "npx jest tests/integration",
        "test:unit": "npx jest --testPathIgnorePatterns=/tests/integration/",
        "test": "npm run test:unit",
        "prepare": "husky install"
    },
    "dependencies": {
        "@mapbox/mapbox-sdk": "^0.4.1",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.9",
        "axios": "^0.21.1",
        "bluebird": "^3.7.2",
        "body-parser": "^1.18.3",
        "connect-history-api-fallback": "^1.6.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.16.3",
        "express-rate-limit": "^5.5.1",
        "faker": "^5.5.3",
        "jest": "^27.0.6",
        "lodash": "^4.17.21",
        "mongo": "^0.1.0",
        "mongoose": "^5.12.7",
        "morgan": "^1.10.0",
        "prettier": "^2.3.2",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "sanitize-html": "^2.6.0",
        "supertest": "^3.4.2"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^2.0.1",
        "@types/jest": "^26.0.24",
        "@types/sanitize-html": "^2.5.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^7.0.0",
        "nodemon": "^2.0.7",
        "semver": "^5.3.0",
        "url-loader": "^1.0.1"
    },
    "engines": {
        "node": "14.17.0",
        "npm": "6.14.0"
    },
    "author": "Nicolas Ramirez",
    "license": "ISC"
}
