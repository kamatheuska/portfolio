        "clean": "rm -rf node_modules",
        "debug": "node --inspect server.js"
    },
    "dependencies": {
        "@mapbox/mapbox-sdk": "^0.4.1",
        "axios": "^0.21.1",
        "bluebird": "^3.5.1",
