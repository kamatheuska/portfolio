{
  "name": "root",
  "private": true,
  "scripts": {
    "start": "npm start --workspace=server",
    "deploy:stage": "git push stage develop:master",
    "deploy:prod": "git push prod master",
    "postinstall": "npm run install:packages",
    "preheroku-prebuild": "printenv | egrep \"NVM|NPM\" && ls -la && ls -la $BUILD_DIR",
    "heroku-prebuild": "npm run install:view && npm run build:view && npm run purge:view",
    "log:files": "find . -maxdepth 2 -mindepth 2",
    "build:view": "npm run build:ci --workspace=landing --workspace=react-projects",
    "install:view": "npm run install:ci --workspace=landing --workspace=react-projects",
    "purge:view": "rm -rf packages/landing packages/react-projects",
    "install:packages": ""
  },
  "engines": {
    "node": "16.14.x",
    "npm": "8.3.x"
  },
  "author": "Nicolas Ramirez",
  "license": "ISC"
}
