{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node init.js",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:integration": "jest -c jest.integration.config.js",
    "test:unit": "jest  -c jest.unit.config.js",
    "lint": "eslint --ext .js \"**/*.js\"",
    "lint:fix": "eslint --ext .js \"**/*.js\" --fix",
    "debug:watch": "nodemon --inspect init.js",
    "debug": "node --inspect=0.0.0.0:9229 init.js",
    "dev:debug-log-mode": "DEBUG_MODE=true nodemon init.js",
    "dev": "nodemon init.js",
    "clean": "rm -rf node_modules",
    "heroku-postbuild": "./scripts/build-view.sh",
    "log:files": "find . -maxdepth 2 -mindepth 2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^5.5.1",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "mongo": "^0.1.0",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.3.2",
    "request": "^2.88.2",
    "sanitize-html": "^2.6.0",
    "supertest": "^3.4.2",
    "validator": "^13.7.0"
