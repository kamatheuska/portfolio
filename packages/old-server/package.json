    "@types/node": "^16.11.9",
    "@types/sanitize-html": "^2.5.0",
    "axios": "^0.21.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7"
